# Server Port (single port for both frontend and API)
PORT=8080

# Gemini API Key for image generation (REQUIRED - get from https://aistudio.google.com/apikey)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Passkey Authentication Service URL (REQUIRED)
VITE_PASSKEY_API_URL=https://passkey.okuso.uk
PASSKEY_API_URL=https://passkey.okuso.uk

# Database Path (optional, defaults to ./data/pixelmap.db)
DB_PATH=./data/pixelmap.db

# Node Environment
NODE_ENV=production